package com.lambda.thread;

public class Book {
	int id;
	String name;
	
	public Book(int id, String name) {
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public void print() {
		return "Book [id=" + id + ", name=" + name + "]";
	}
	
}
