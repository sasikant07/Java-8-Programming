package com.lambda.thread;

public class LambdaThreadMain {

	public static void main(String[] args) {
		LambdaClass lc = new LambdaClass();
		
		Thread thread = new Thread( () -> {
				for(int i=1; i <= 5; i++) {
		            System.out.println("Child Thread: "+ i);
		            try {
		               Thread.sleep(500);
		            } catch(Exception e) {
		               e.printStackTrace();
		            }
		         }
		}).start();
		
		
		
	}

}
